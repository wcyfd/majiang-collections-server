<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.randioo.majiang_collections_server.dao.ConsumeDao">
	<insert id="insert">
		insert into consume(
			roleId,
			money,
			time
		) values (
			#{roleId},
			#{money},
			now()
		)
	</insert>
</mapper>